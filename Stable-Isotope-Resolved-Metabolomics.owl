    <!-- http://www.semanticweb.org/prs/ontologies/2013/11/Stable-Isotope-Resolved-Metabolomics.owl#&apos;2D_NMR_spectroscopy&apos; -->

    <Class rdf:about="&Stable-Isotope-Resolved-Metabolomics;&apos;2D_NMR_spectroscopy&apos;">
        <rdfs:label xml:lang="en">2D NMR spectroscopy</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;OBI_0000011"/>
        <obo:IAO_0000117 xml:lang="en">Philippe Rocca-Serra</obo:IAO_0000117>
        <obo:IAO_0000115 xml:lang="en">tow dimensional nuclear magnetic resonance is a kind of spectroscopy technique relying on electron spin variation in response to high intensity magnetic fields and recording of atomic resonance in response to specific external frequency stimulation in the form of pulse sequence, providing information about chemical entities and their structure as found in a sample. In contrast to 1D NMR, 2D NMR relies on pulse sequences to generate echoes from 2 distinct nuclei, identified as acquisition nuclei</obo:IAO_0000115>
    </Class>
    


    <!-- http://www.semanticweb.org/prs/ontologies/2013/11/Stable-Isotope-Resolved-Metabolomics.owl#&apos;exponentially_fed-batch_culture&apos; -->

    <Class rdf:about="&Stable-Isotope-Resolved-Metabolomics;&apos;exponentially_fed-batch_culture&apos;">
        <rdfs:label xml:lang="en">exponentially fed-batch culture reactor</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;OBI_0000967"/>
        <obo:IAO_0000118 xml:lang="en">EFBC</obo:IAO_0000118>
        <obo:IAO_0000117 xml:lang="en">Philippe Rocca-Serra</obo:IAO_0000117>
        <obo:IAO_0000115 xml:lang="en">an exponentially fed-batch culture reactor is a device which deliver the ability to grow cells in a way non too dissimilar to that of a chemostat based culture, meaning that the cells are in a quasi steady-state condition.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">http://onlinelibrary.wiley.com/doi/10.4319/lom.2014.12.432/epdf</obo:IAO_0000119>
    </Class>
    


    <!-- http://www.semanticweb.org/prs/ontologies/2013/11/Stable-Isotope-Resolved-Metabolomics.owl#&apos;full_scan_data_acquisition&apos; -->


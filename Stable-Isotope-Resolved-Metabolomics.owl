    <!-- http://www.semanticweb.org/prs/ontologies/2013/11/Stable-Isotope-Resolved-Metabolomics.owl#metabolism_quenching -->

    <Class rdf:about="&Stable-Isotope-Resolved-Metabolomics;metabolism_quenching">
        <rdfs:label xml:lang="en">metabolism quenching</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;OBI_0000094"/>

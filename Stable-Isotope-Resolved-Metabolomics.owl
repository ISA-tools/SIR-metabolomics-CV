                <onProperty rdf:resource="&obo;IAO_0000136"/>
                <onClass rdf:resource="&Stable-Isotope-Resolved-Metabolomics;chemical_reaction"/>
                <minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</minQualifiedCardinality>
